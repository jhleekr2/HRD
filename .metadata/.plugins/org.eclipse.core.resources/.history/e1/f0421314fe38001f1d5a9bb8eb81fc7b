package java04_control.loop;

public class While_03 {
	
		int i = 0;
		int j = 0;
		//QUIZ
		//구구단 2-9단을 출력하기
		for(i=1;i<10;i++) {
			System.out.println();
			System.out.print(j + "X" + i + "=" + (j*i) + "\t");			
			}		
		
		
		while ( i=2 ) {
			System.out.print(j + "X" + i + "=" + (j*i) + "\t")) ;
		}
	}

}


