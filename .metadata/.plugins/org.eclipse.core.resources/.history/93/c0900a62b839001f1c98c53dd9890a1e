package java05_array;

public class Array_02 {
	public static void main(String[] args) {
		
		int[] arr1;
		arr1 = new int[7];
		
		//----------------------------------------------------------------
		
		int[] arr2 = new int[13];
//		int arr2[] = new int[13];
//		int                   []                  arr3            = new int            [13]; // 가능하다
				
		for(int i=0; i<13; i++) {
			System.out.println(i + " 인덱스 : " + arr2[i]);
		}
	}
}
