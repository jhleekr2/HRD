package java05_array.sort;

import java.util.Arrays;

public class Sort {
	public static void main(String[] args) {
		
		int[] arr = {5,34,7,23,2,1,23,6,57};
		
//		String str = Arrays.toString(arr);
		
		//배열 출력
		System.out.println( Arrays.toString(arr));
		
		//정렬
		Arrays.sort(arr);
		
		//배열 출력
		System.out.println( Arrays.toString(arr));
	}
}
