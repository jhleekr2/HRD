package java04_control_star;

public class Star_10 {
	public static void main(String[] args) {
		
		int i = 0;
		int j = 0;
		
		for(i=0 ; i<5 ; i++) {
				for(j=0; j<i; j++) {
					System.out.print(" ");
			}
				for(j=0; i+j>5; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
			
	}
}

//i행 j열에서 j<i+1일때까지 공백을 우선 출력
//이후  