package java04_control.selection;

public class Switch_02 {
	public static void main(String[] args) {
		
		int num = 30;
		
		if( num == 10 ) {
			System.out.println("10입니다");

		} else if( num == 20 ) {
			System.out.println("20입니다");

		} else if( num == 30 ) {
			System.out.println("30입니다");

		} else {
			System.out.println("10, 20, 30 모두 아닙니다");

		}
	}
}
