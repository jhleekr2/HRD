package java04_control.loop;

public class While_03 {
	public static void main() {
		int i = 1;
		int j = 2;
		//QUIZ
		//구구단 2-9단을 출력하기
//		for(i=1;i<10;i++) {
//			System.out.println();
//			System.out.print(j + "X" + i + "=" + (j*i) + "\t");			
//			}		
//		
		while (j<=9) {	
			while (i<=9) {
				System.out.print(j + "X" + i + "=" + (j*i) + "\t");			

				i++; //증감식
			}
			System.out.println();
		}
	}
}

