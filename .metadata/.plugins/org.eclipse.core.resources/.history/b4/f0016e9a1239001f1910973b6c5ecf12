package java04_control.loop;

public class LabeledBreak {
	public static void main(String[] args) {
		
		//레이블 브레이크(Labeled Break)
		//레이블 컨티뉴(Labeled Continue)
		
		Apple:
		for(int i=0; i<5; i++) {
			
			Banana:
			for(int j=0; j<5; j++) {
				
				if( j>=2 ) {
					break Banana;
				}
				
				System.out.println("i:" + i + ", j:" + j);
			}
		}
	}
}
