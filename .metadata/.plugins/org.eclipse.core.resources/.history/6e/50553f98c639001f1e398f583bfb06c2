package java05_array;

public class Array_03 {
	public static void main(String[] args) {
		
		int[] arr = {47,8,2,3,9,5,79,8235,78,9,3,2058,329,06,802,986,9,238,6};
		
		//** 배열의 길이
		// 요쇼의 개수
		// 배열의 크기
		
		// -> 배열이름.length
		
		// ex) arr.length
		//--------------------------------------------------------
		
		System.out.println("배열의 길이 : " + arr.length);
		
		System.out.println("------------");
		
	}
}
