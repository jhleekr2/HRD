package java03_scanner;

public class Unary_01 {
	public static void main(String[] args) {
		
		// 단항 연산자 - 부호, sign
		// + -
		//---------------------------------------------------
		
		int num1 = -7; //음수
		int num2 = +7; //양수
		
		System.out.println( -num1 );
//		System.out.println( -(-7) );
//		System.out.println( 7 );
		
		//---------------------------------------------------
		
		// ! NOT, 논리 부정 연산자
		// ~ bit NOT, 비트단위 부정 연산자
		
		// (type) 형변환 연산자
		
		// . 참조(reference) 연산자
	
	}
}
